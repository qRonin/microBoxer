@code {
    [Parameter]
    public EventCallback<string> actionResult { get; set; }
    public async Task ReturnClicked()
    {
        actionResult.InvokeAsync("ReturnClicked");
    }
    public async Task AddClicked()
    {
        actionResult.InvokeAsync("AddClicked");
    }
    public async Task ScanClicked()
    {
        actionResult.InvokeAsync("ScanClicked");
    }
}
